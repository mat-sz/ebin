  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "binary",
    "parser",
    "serializer",
    "library",
    "typescript"
  ],
  "scripts": {
    "prepare": "npm run build",
    "test": "jest",
    "build": "tsc",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mat-sz/ebin.git"
  },
  "bugs": {
    "url": "https://github.com/mat-sz/ebin/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
